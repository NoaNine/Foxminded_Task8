<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Turquoise">
        #81d6c1
    </Color>
    <Color x:Key="DarkTurquoise">
        #419297
    </Color>

    <SolidColorBrush x:Key="TurquoiseBrush" 
                     Color="{StaticResource Turquoise}"/>
    <SolidColorBrush x:Key="DarkTurquoiseBrush" 
                     Color="{StaticResource DarkTurquoise}"/>
    
    <Image x:Key="ImageArrowLeft" 
           Source="/Files/Images/arrowleft.png"/>
    <Image x:Key="ImageArrowRight" 
           Source="/Files/Images/arrowright.png"/>
</ResourceDictionary>