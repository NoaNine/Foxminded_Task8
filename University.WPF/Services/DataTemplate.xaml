<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:controls="clr-namespace:University.WPF.View.Controls" 
                    xmlns:viewModel="clr-namespace:University.WPF.ViewModel" 
                    xmlns:viewGroup="clr-namespace:University.WPF.View.Group"
                    xmlns:viewHome="clr-namespace:University.WPF.View.Home">

    <DataTemplate DataType="{x:Type TypeName=viewModel:SectionBarViewModel}">
        <controls:SectionBar/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=viewModel:BreadcrumbBarViewModel}">
        <controls:BreadcrumbBar/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=viewModel:GroupViewModel}">
        <viewGroup:GroupView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=viewModel:HomeViewModel}">
        <viewHome:HomeView/>
    </DataTemplate>
</ResourceDictionary>